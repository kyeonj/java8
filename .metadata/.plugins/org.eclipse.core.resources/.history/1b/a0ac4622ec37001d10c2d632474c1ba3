package sec1;

public class MemberEx {

	public static void main(String[] args) {
		//클래스명 
		Member mem1 = new Member();
		mem1.setId("kkt");
		System.out.println("아이디 : "+mem1.getId());
		//mem1.setPassword("3949");
		System.out.println("비밀번호 : "+mem1.getPassword());
		//mem1.setName("연정");
		System.out.println("이름: "+mem1.getName());
		mem1.setAddress("백석동");
		System.out.println("주소 : "+mem1.getAddress());
		
		Member mem2 = new Member();
		mem2.setId("KYJ");
		System.out.println("멤버2의 아이디 : "+mem2.getId());
		
		Member mem3 = new Member();
		
		MemberContoller mc = new MemberContoller();
		mc.join("kyj", "1004", "김연정");
		mc.info();
		mc.login("kyj", "1004");
		mc.info();
		
		
	}
}
