package sec2;

import java.util.Scanner;

public class Condition1 {

	public static void main(String[] args) {
		System.out.println("점수입력:");
		Scanner sc = new Scanner(System.in);//컨트롤쉬프트o누르면생김
		int num = sc.nextInt();
		if(num>=80) {
			System.out.println("합격");
		} else {
			System.out.println("불합격");
		}
		sc.close();
	}

}
