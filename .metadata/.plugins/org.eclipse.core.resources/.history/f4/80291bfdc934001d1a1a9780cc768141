package sec4;

public class Ex6 {

	public static void main(String[] args) {
		for(int a=0;a<5;a++){
			for(int b=0;b<=a;b++){
				System.out.print("*");
			}
				System.out.println();
			System.out.println("\t");
			
			
			for(int a1=0;a1<5;a1++){
				for(int b=0;b<5-a1;b++){
					System.out.print("*");
				}
			}
		}
	}

}


